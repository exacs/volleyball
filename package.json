    "url": "https://github.com/exacs/volleyball/issues"
  },
  "homepage": "https://github.com/exacs/volleyball#readme",
  "devDependencies": {
    "babel-plugin-istanbul": "^2.0.3",
    "babel-register": "^6.18.0",
    "chai": "^3.5.0",
    "mocha": "^3.1.2"
  },
  "dependencies": {
    "babel-preset-es2015": "^6.18.0"
  }
}
